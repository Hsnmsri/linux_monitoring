mkdir -p src/build
cp src/assets/settings.json src/build -n
g++ -I src/library src/main.cpp src/library/Log.cpp src/library/Settings.cpp src/library/CpuMonitor.cpp src/library/MemoryMonitor.cpp -o src/build/LinuxMonitoring -pthread -lcurl --std=c++14 -I/usr/local/include -lTgBot -lboost_system -lssl -lcrypto -lpthread 
echo "compiled to src/build directory."